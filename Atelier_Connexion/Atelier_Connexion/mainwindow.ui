}

QTabWidget::pane { /* The tab widget frame */
    border: none; /* Remove the border around the pane */
}

QTabBar::tab {
    height: 40px; /* Standard height for tabs */
    width: 177px; /* Standard width for tabs */
    padding: 5px; /* Padding around the tab text */
    margin: 0px; /* No margin around the tabs */
    text-align: center; /* Center the text alignment */
    background-color: #e0e0e0; /* Light gray background for inactive tabs */
    border: 1px solid #ccc; /* Light border for tabs */
    border-bottom: none; /* No bottom border to blend with the pane */
    border-radius: 5px 5px 0 0; /* Rounded corners for the top of the tabs */
}

QTabBar::tab:selected {
    background-color: #ffffff; /* White background for the selected tab */
    border: 1px solid #a0a0a0; /* Darker border for selected tab */
    border-bottom: 1px solid #ffffff; /* Match the pane to blend */
}

QTabBar::tab:hover {
    background-color: #d0d0d0; /* Slightly darker gray for hovered tabs */
}

/* Optional: Adjust font weight for selected tab */
QTabBar::tab:selected {
    font-weight: bold; /* Bold text for the selected tab */
}
